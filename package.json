{
  "name": "edupathnguyenvanthoai",
  "author": "NguyenVanThoaiSchoolApp",
  "licence": "MIT",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "bun --bun vite",
    "start": "bun --bun vite preview",
    "build": "bun --bun vite build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:print": "bunx eslint --print-config eslint.config.mjs > eslint-show-config.json",
    "fm:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "fm:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "fix:all": "bun run lint:fix && bun run fm:fix",
    "clean": "rm -rf node_modules .next out dist build",
    "re:dev": "yarn clean && yarn install && yarn dev",
    "re:build": "yarn clean && yarn install && yarn build",
    "re:build-npm": "bun run clean && bun install && bun run build",
    "tsc:dev": "yarn dev & yarn tsc:watch",
    "tsc:watch": "tsc --noEmit --watch",
    "tsc:print": "npx tsc --showConfig"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "bun@1.2.22",
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource-variable/dm-sans": "^5.2.8",
    "@fontsource/barlow": "^5.2.8",
    "@iconify/react": "^5.2.1",
    "@mui/lab": "^7.0.0-beta.17",
    "@mui/material": "^7.3.2",
    "ahooks": "^3.9.5",
    "apexcharts": "^4.7.0",
    "dayjs": "^1.11.18",
    "dexie": "^4.2.0",
    "dexie-react-hooks": "^4.2.0",
    "es-toolkit": "^1.39.10",
    "minimal-shared": "^1.0.15",
    "react": "^19.1.1",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "react-router-dom": "^7.9.2",
    "simplebar-react": "^3.3.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^22.18.6",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "eslint": "^9.36.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^6.3.6",
    "vite-plugin-checker": "^0.9.3"
  }
}
